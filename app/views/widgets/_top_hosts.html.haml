.panel.panel-danger
  .panel-heading.center
    %strong Top 5 Hosts
  .panel-body
    %table.table
      %thead
        %th IP Address
        %th.center Count
      %tbody
        - top_ten = get_top_hosts_critical(obj)
        - top_ten.each do |k,v|
          - affected_host = obj.where(host_ip: k).first
          - if !affected_host.nil?
            %tr
              -# %td= link_to k, show_vulnerability_path(host, project_group_id: params[:id])
              %td= link_to k, project_group_affected_host_vulnerabilities_path(project_group, affected_host)
              %td.center= v
