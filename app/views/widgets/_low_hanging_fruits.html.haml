- low_hanging_fruits = get_low_hanging_fruits(obj)
%table.table
  %thead
    %tr
      %th Vulnerability Name
      %th.center Count

  %tbody
    - low_hanging_fruits.each do |key, value|
      %tr
        %td
          = link_to key, list_affected_hosts_path(plugin_id: get_plugin_id_by_name(obj, key),project_group_id: @project_group)
        %td.center= value
